<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".retail.StoresNavBar">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/stores_app_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/HermesCustomTheme.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/stores_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:fitsSystemWindows="true"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/HermesCustomTheme.NoActionBar" />

    </com.google.android.material.appbar.AppBarLayout>

    <include layout="@layout/content_stores_nav_bar" />


</androidx.coordinatorlayout.widget.CoordinatorLayout>